{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "parcel serve public/index.html",
    "build": "yarn run clean && parcel build --no-scope-hoist public/index.html",
    "build:dev": "yarn run build --dist-dir ../cmd/web/assets",
    "clean": "yarn run rimraf ../cmd/web/assets",
    "lint": "eslint",
    "format:eslint": "eslint '*.cjs' 'src/**/*.{ts,vue}' --fix",
    "format:prettier": "prettier '*.cjs' 'src/**/*.{ts,vue}' --write"
  },
  "dependencies": {
    "vue": "^3.1.2",
    "vue-i18n": "^9.1.2",
    "vue-router": "^4.0.6",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-typescript": "^7.14.6",
    "@parcel/babel-plugin-transform-runtime": "^2.0.0",
    "@parcel/babel-preset-env": "^2.0.0",
    "@parcel/transformer-vue": "^2.0.0",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "autoprefixer": "^10.2.5",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^7.8.0",
    "lint-staged": "^11.2.1",
    "parcel": "^2.0.0",
    "postcss": "^8.1.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "typescript": "~4.2.4"
  }
}
